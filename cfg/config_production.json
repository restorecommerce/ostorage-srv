{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "node": "http://elasticsearch:9200"
      },
      "dataStream": true,
      "source": "ostorage-srv"
    }
  },
  "redis": {
    "url": "redis://redis:6379"
  },
  "s3": {
    "client": {
      "endpoint": "http://cloudserver:8000"
    }
  },
  "events": {
    "kafka": {
      "kafka": {
        "brokers": [
          "kafka:9092"
        ]
      }
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpcOstorage",
        "provider": "grpc",
        "addr": "0.0.0.0:50051"
      }
    ]
  },
  "client": {
    "acs-srv": {
      "address": "access-control-srv:50051"
    },
    "user": {
      "address": "identity-srv:50051"
    }
  },
  "authorization": {
    "cache":  {
      "url": "redis://redis:6379"
    }
  }
}
