  "dependencies": {
    "@restorecommerce/chassis-srv": "github:restorecommerce/chassis-srv",
    "@restorecommerce/cluster-service": "^0.1.0",
    "@restorecommerce/kafka-client": "github:restorecommerce/kafka-client",
    "@restorecommerce/logger": "github:restorecommerce/logger",
    "@restorecommerce/protos": "github:restorecommerce/protos",
    "@restorecommerce/resource-base-interface": "github:restorecommerce/resource-base-interface",
    "@restorecommerce/service-config": "^0.1.0",
    "aws-sdk": "^2.326.0",
    "cross-env": "^5.2.0",
    "koa-multer": "^1.0.2",
    "memorystream": "^0.3.1",
    "multer-s3": "^2.9.0",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.0.0",
    "@types/node": "^10.3.5",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^5.1.0",
    "nodemon": "^1.12.1",
    "should": "^13.2.1"
  },
  "scripts": {
    "start": "node start.js",
    "dev": "cross-env NODE_ENV=development nodemon start.js",
