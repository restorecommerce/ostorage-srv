    "@restorecommerce/resource-base-interface": "^1.2.5",
    "@restorecommerce/service-config": "^1.0.6",
    "aws-sdk": "^2.1424.0",
    "koa-multer": "^1.0.2",
    "multer-s3": "^3.0.1",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.0.21",
    "@grpc/proto-loader": "^0.7.8",
