{
  "name": "@restorecommerce/ostorage-srv",
  "version": "0.1.18",
  "description": "Restorecommerce microservice for storing objects",
  "main": "lib/start.js",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/ostorage-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ostorage",
    "srv"
  ],
  "dependencies": {
    "@restorecommerce/acs-client": "^0.2.23",
    "@restorecommerce/chassis-srv": "^0.1.11",
    "@restorecommerce/cluster-service": "^0.1.4",
    "@restorecommerce/grpc-client": "^0.0.9",
    "@restorecommerce/kafka-client": "^0.1.13",
    "@restorecommerce/logger": "^0.6.4",
    "@restorecommerce/protos": "^0.0.22",
    "@restorecommerce/resource-base-interface": "^0.0.7",
    "@restorecommerce/service-config": "^0.4.1",
    "aws-sdk": "^2.848.0",
    "koa-multer": "^1.0.2",
    "multer-s3": "^2.9.0",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.20.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "grpc-mock": "^0.7.0",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "sleep": "^6.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "start": "npm run createtopics && node lib/start.js",
    "dev": "cross-env NODE_ENV=development npm run createtopics && nodemon -e ts --trace-warnings lib/start.js",
    "pretest": "npm run build && npm run tsctests && npm run createtopics",
    "test": "npm run lint && nyc npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha --timeout 15000 -R spec test/test.js --full-trace --exit",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test-debug": "cross-env NODE_ENV=test mocha -R spec test/test.js --inspect-brk",
    "lcov-report": "nyc report --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "createtopics": "node setupTopics.js io.restorecommerce.command io.restorecommerce.ostorage",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc",
    "healthcheck": "node lib/healthcheck.js"
  },
  "engines": {
    "node": ">= 12.0.0"
  }
}
