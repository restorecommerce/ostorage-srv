  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ostorage",
    "srv"
  ],
  "dependencies": {
    "@restorecommerce/acs-client": "^0.4.13",
    "@restorecommerce/chassis-srv": "^0.3.2",
    "@restorecommerce/cluster-service": "^0.1.10",
    "@restorecommerce/grpc-client": "^0.2.10",
    "@restorecommerce/kafka-client": "^0.2.23",
    "@restorecommerce/logger": "^0.10.8",
    "@restorecommerce/protos": "^0.4.5",
    "@restorecommerce/resource-base-interface": "^0.2.0",
    "@restorecommerce/service-config": "^0.4.18",
    "aws-sdk": "^2.988.0",
    "koa-multer": "^1.0.2",
    "multer-s3": "^2.9.0",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "grpc-mock": "^0.7.0",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "sleep": "^6.3.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "node lib/start.js",
    "dev": "cross-env NODE_ENV=development && nodemon -e ts --trace-warnings lib/start.js",
    "test": "npm run build && npm run lint && nyc npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha --full-trace --exit --timeout 15000",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test-debug": "cross-env NODE_ENV=test mocha -R spec test/test.js --inspect-brk",
    "lcov-report": "nyc report --reporter=lcov",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
