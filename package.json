{
  "name": "@restorecommerce/ostorage-srv",
  "version": "0.0.1",
  "description": "Restore Commerce microservice for storing objects",
  "main": "start.js",
  "author": "Invend GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/ostorage-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ostorage",
    "srv"
  ],
  "dependencies": {
    "@restorecommerce/chassis-srv": "github:restorecommerce/chassis-srv",
    "@restorecommerce/cluster-service": "^0.1.0",
    "@restorecommerce/grpc-client": "github:restorecommerce/grpc-client",
    "@restorecommerce/kafka-client": "github:restorecommerce/kafka-client",
    "@restorecommerce/logger": "^0.1.8",
    "@restorecommerce/protos": "github:restorecommerce/protos",
    "@restorecommerce/resource-base-interface": "github:restorecommerce/resource-base-interface",
    "@restorecommerce/service-config": "^0.1.0",
    "aws-sdk": "^2.491.0",
    "koa-multer": "^1.0.2",
    "memorystream": "^0.3.1",
    "multer-s3": "^2.9.0",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.2",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "should": "^13.2.3",
    "sleep": "^6.1.0",
    "tslint": "^5.18.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "node start.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch './*.ts' --exec 'ts-node' start.ts",
    "pretest": "npm run tsctests",
    "test": "npm run lint && npm run mocha",
    "lint": "tslint ./start.ts ./worker.ts ./oStorageService.ts ./commandInterface.ts",
    "mocha": "cross-env NODE_ENV=test mocha --timeout 15000 -R spec test/test.js --full-trace --exit",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test-debug": "cross-env NODE_ENV=test mocha -R spec test/test.js --inspect-brk",
    "coveralls": "cross-env NODE_ENV=test; nyc --reporter text --reporter lcov npm test",
    "postinstall": "tsc -d",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf *.js",
    "build": "npm-run-all lint build:clean build:tsc"
  },
  "engines": {
    "node": ">= 9.2.0"
  }
}
