{
  "name": "@restorecommerce/ostorage-srv",
  "version": "1.2.3",
  "description": "Restorecommerce microservice for storing objects",
  "main": "lib/start.js",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/ostorage-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ostorage",
    "srv"
  ],
  "type": "module",
  "dependencies": {
    "@restorecommerce/acs-client": "^1.4.12",
    "@restorecommerce/chassis-srv": "^1.4.8",
    "@restorecommerce/cluster-service": "^1.0.2",
    "@restorecommerce/grpc-client": "^2.1.1",
    "@restorecommerce/kafka-client": "^1.1.20",
    "@restorecommerce/logger": "^1.2.9",
    "@restorecommerce/rc-grpc-clients": "^5.1.20",
    "@restorecommerce/resource-base-interface": "^1.4.7",
    "@restorecommerce/service-config": "^1.0.11",
    "aws-sdk": "^2.1568.0",
    "koa-multer": "^1.0.2",
    "lodash-es": "^4.17.21",
    "multer-s3": "^3.0.1",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.1.9",
    "@grpc/proto-loader": "^0.7.10",
    "@restorecommerce/dev": "^0.0.8",
    "@restorecommerce/protos": "^6.7.0",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.23",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "c8": "^9.1.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prefer-arrow-functions": "^3.3.2",
    "mocha": "^10.3.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.5",
    "should": "^13.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "start": "node lib/start.cjs",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec node --loader ts-node/esm ./src/start.ts",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test": "npm run lint && c8 --reporter=text npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha --full-trace --exit --timeout 15000",
    "test-debug": "cross-env NODE_ENV=test mocha -R spec test/test.js --inspect-brk",
    "lcov-report": "c8 report --reporter=lcov",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "build:tsc": "esbuild ./src/start.ts --bundle --platform=node --outfile=lib/start.cjs --minify --tree-shaking=true --sourcemap=inline",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc"
  },
  "engines": {
    "node": ">= 20.8.0"
  }
}
