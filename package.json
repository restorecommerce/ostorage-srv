{
  "name": "@restorecommerce/ostorage-srv",
  "version": "1.2.3",
  "description": "Restorecommerce microservice for storing objects",
  "main": "lib/start.js",
  "author": "n-fuse GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/ostorage-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "ostorage",
    "srv"
  ],
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@restorecommerce/acs-client": "^3.1.6",
    "@restorecommerce/chassis-srv": "^1.7.8",
    "@restorecommerce/grpc-client": "^2.2.13",
    "@restorecommerce/kafka-client": "^1.3.6",
    "@restorecommerce/logger": "^1.3.8",
    "@restorecommerce/rc-grpc-clients": "^5.1.63",
    "@restorecommerce/resource-base-interface": "^1.9.8",
    "@restorecommerce/service-config": "^1.1.7",
    "koa-multer": "^1.0.2",
    "lodash-es": "^4.17.21",
    "multer-s3": "^3.0.1",
    "s3-upload-stream": "^1.0.7",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@alenon/grpc-mock-server": "^3.2.3",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@grpc/proto-loader": "^0.8.0",
    "@restorecommerce/dev": "^0.2.0",
    "@restorecommerce/protos": "^6.11.11",
    "@semantic-release-plus/docker": "^3.1.3",
    "@types/lodash-es": "^4.17.12",
    "@types/mocha": "10.0.10",
    "@types/node": "^24.9.1",
    "c8": "^10.1.2",
    "coveralls": "^3.1.1",
    "cross-env": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "husky": "9.1.7",
    "mocha": "^11.7.4",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "nyc": "17.1.0",
    "rimraf": "6.0.1",
    "should": "^13.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "scripts": {
    "start": "node lib/start.cjs",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec node --loader ts-node/esm ./src/start.ts",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test": "npm run lint && c8 --reporter=text npm run mocha",
    "lint": "eslint src",
    "mocha": "cross-env NODE_ENV=test mocha --full-trace --exit --timeout 15000",
    "test-debug": "cross-env NODE_ENV=test mocha -R spec test/test.js --inspect-brk",
    "lcov-report": "c8 report --reporter=lcov",
    "coveralls": "c8 report --reporter=text-lcov | coveralls",
    "build:es": "node esbuild.mjs",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:es",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {}\""
  },
  "engines": {
    "node": ">= 20.8.0"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      [
        "@semantic-release-plus/docker",
        {
          "name": "docker.io/restorecommerce/ostorage-srv",
          "skipLogin": true
        }
      ],
      [
        "@semantic-release-plus/docker",
        {
          "name": "ghcr.io/restorecommerce/ostorage-srv",
          "skipLogin": true
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
